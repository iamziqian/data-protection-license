{
  "name": "data-protection-license",
  "version": "1.0.0",
  "description": "A machine-readable licensing protocol that enables content creators to protect their datasets from unauthorized AI training",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/api/validation-server.js",
    "dev": "nodemon src/api/validation-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "babel src -d dist",
    "docker:build": "docker build -t data-protection-license .",
    "docker:run": "docker run -p 3000:3000 data-protection-license",
    "setup:db": "psql -f database/schema.sql",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "monitor": "node src/monitoring/compliance-monitor.js",
    "deploy": "node scripts/deploy.js"
  },
  "keywords": [
    "data-protection",
    "license",
    "ai-training",
    "copyright",
    "compliance",
    "gdpr",
    "ccpa",
    "json-ld",
    "cryptography",
    "blockchain",
    "validation"
  ],
  "author": "Software Engineer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.8",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "prom-client": "^14.2.0",
    "axios": "^1.5.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.0",
    "joi": "^17.10.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "csv-parser": "^3.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/cli": "^7.22.15",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/data-protection-license.git"
  },
  "bugs": {
    "url": "https://github.com/username/data-protection-license/issues"
  },
  "homepage": "https://github.com/username/data-protection-license#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "max-len": ["error", { "code": 120 }]
    }
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
